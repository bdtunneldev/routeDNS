{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "id": 1,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Current Connections",
      "type": "stat",
      "targets": [{ "expr": "haproxy_frontend_current_sessions{proxy=\"ft_dot\"}", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Requests/sec",
      "type": "stat",
      "targets": [{ "expr": "rate(haproxy_frontend_sessions_total{proxy=\"ft_dot\"}[1m])", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Backend Servers Up",
      "type": "stat",
      "targets": [{ "expr": "sum(haproxy_backend_active_servers)", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "percentunit", "min": 0, "max": 1, "color": { "mode": "thresholds" }, "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 0.95 }, { "color": "green", "value": 0.99 }] } }
      },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "title": "Backend Uptime",
      "type": "stat",
      "targets": [{ "expr": "avg(haproxy_backend_up)", "refId": "A" }]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Frontend Connections Over Time",
      "type": "timeseries",
      "targets": [
        { "expr": "haproxy_frontend_current_sessions{proxy=\"ft_dot\"}", "legendFormat": "Current Sessions", "refId": "A" },
        { "expr": "rate(haproxy_frontend_sessions_total{proxy=\"ft_dot\"}[5m])", "legendFormat": "Session Rate (5m)", "refId": "B" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "decbytes", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 6,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Traffic (Bytes)",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(haproxy_frontend_bytes_in_total{proxy=\"ft_dot\"}[5m])", "legendFormat": "Bytes In", "refId": "A" },
        { "expr": "rate(haproxy_frontend_bytes_out_total{proxy=\"ft_dot\"}[5m])", "legendFormat": "Bytes Out", "refId": "B" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 7,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Connection Errors & Denials",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(haproxy_frontend_request_errors_total{proxy=\"ft_dot\"}[5m])", "legendFormat": "Request Errors", "refId": "A" },
        { "expr": "rate(haproxy_frontend_denied_connections_total{proxy=\"ft_dot\"}[5m])", "legendFormat": "Denied Connections", "refId": "B" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "s", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "title": "Backend Response Time",
      "type": "timeseries",
      "targets": [
        { "expr": "haproxy_backend_response_time_average_seconds", "legendFormat": "{{proxy}}", "refId": "A" }
      ]
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": { "unit": "short", "color": { "mode": "palette-classic" } }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
      "id": 9,
      "options": { "legend": { "displayMode": "table", "placement": "right" }, "tooltip": { "mode": "multi" } },
      "title": "Rate Limiting - Connections per IP",
      "type": "timeseries",
      "targets": [
        { "expr": "rate(haproxy_frontend_denied_connections_total[5m])", "legendFormat": "Denied (Rate Limited)", "refId": "A" },
        { "expr": "rate(haproxy_frontend_sessions_total[5m])", "legendFormat": "Total Sessions", "refId": "B" }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["haproxy", "dns", "dot"],
